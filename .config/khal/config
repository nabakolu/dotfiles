[calendars]

[[calendar_feiertage]]
path = ~/calendars/feiertage/
type = calendar
color = dark blue
readonly = true

[[calendar_league]]
path = ~/calendars/league/
type = calendar
color = dark red
readonly = true

[[calendar_personal]]
path = ~/calendars/personal
type = calendar
color = light green

[[birthdays]]
path = ~/contacts/contacts
type = birthdays
color = white

[locale]
timeformat = %H:%M
dateformat = %d/%m/%Y
longdateformat = %d/%m/%Y
datetimeformat = %d/%m/%Y %H:%M
longdatetimeformat = %d/%m/%Y %H:%M
unicode_symbols = False

[default]
default_calendar = calendar_personal
highlight_event_days = true
timedelta = 1d

[view]
frame = width
dynamic_days = false
